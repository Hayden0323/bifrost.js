        "type": "(CurrencyId, NumberOrHex)",
        "isSubscription": false,
        "jsonrpc": "chargeTransactionFee_getFeeTokenAndAmount",
        "method": "getFeeTokenAndAmount",
        "section": "chargeTransactionFee"
      }
